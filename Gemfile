# Works for Ruby 1.9.3 / Mac / MSSQL Server 2012
gem 'ruby-odbc', '0.99994'                          # version 0.99998 works also
gem 'tiny_tds', '0.4.5'
gem 'activerecord', '3.2.13'
gem 'activerecord-sqlserver-adapter', '3.2.10'     # MS SQL SERVER 2005, 2008, 2010, 2011, 2012 are supported

